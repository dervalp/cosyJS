{
  "name": "cosy",
  "description": "module to manage your static resources, full-stack UI component library, layout generator",
  "version": "0.0.7d",
  "author": "Pierre Derval <dervalp@gmail.com>",
  "dependencies": {
    "jade": "*",
    "async": "*",
    "backbone": "*",
    "browserify": "*",
    "handlebars": "*",
    "underscore": "*",
    "less": "*",
    "rivets": "*"
  },
  "devDependencies": {
    "mocha": "~1.11.0",
    "grunt": "*",
    "grunt-contrib-less": "~0.5.1",
    "should": "*",
    "blanket": "~1.1.5",
    "travis-cov": "~0.2.4",
    "grunt-contrib-jshint": "~0.6.0",
    "grunt-shell-spawn": "~0.2.4",
    "grunt-contrib-watch": "~0.4.4"
  },
  "scripts": {
    "travis-cov": {
      "threshold": 70
    },
    "blanket": {
      "pattern": [
        "lib/core/",
        "lib/utils/"
      ]
    },
    "test": "mocha --recursive --require blanket -R travis-cov"
  },
  "repository": "git://github.com/dervalp/cosyJS",
  "main": "index",
  "engines": {
    "node": ">=0.8"
  }
}
